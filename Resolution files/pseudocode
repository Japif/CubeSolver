		case QJC: U2 oll 19
		case QJF: U oll 2
		case QJI: oll 1
		case QMC: U oll 4
		case QMF: oll 2
		case QMI: oll 4

//controllo headlights nel pll (capire come vanno scambiati gli angoli)

if (N==M)
{
	if (J==I)
		tipologia=EPLL
	else
		tipologia=adj
		esegui U’
}
else
{
	if (J==I)
		tipologia=adj
		esegui U2
	else
	{	
		if (F==H)
			tipologia=adj
			esegui U
		else
		{
			if (Q==R)
				tipologia=adj
			else
				tipologia=diag
		}
	}
}

//pll vero e proprio	*le lettere che hanno un asterisco si riferiscono agli spigoli, le altre agli angoli
			**quando dico M*==-N vuol dire “lo spigolo M ha il colore opposto 				all’angolo N?” (blu opposto a verde e rosso opposto ad arancione)
			***vuol dire “lo spigolo M è diverso dall’angolo N e anche dall’opposto di 			N?” (in pratica “lo spigolo M è adiacente a N?”)

if (tipologia==adj)
{
	if (Q*==Q)
	{
		if (M*==N)
			esegui Jb
		if (M*==-N)**
			esegui F
		if (M*!=N && M*!=-N)***
			esegui Ja
	}
	if (Q*==-Q)
	{
		if (M*==N)
			esegui T
		if (M*==-N)
			esegui Gd
		if (M*!=N && M*!=-N)
			esegui Gb
	}
	if (Q*!=Q && Q*!=-Q)
	{
		if (M*==N)
			esegui Rb
		if (M*==-N)
			esegui Ga
		if (M*!=N && M*!=-N)
		{
			if (I*==J)
				esegui Ab
			if (I*==-J)
				esegui Aa
			if (I*!=J && I*!=-J)
			{
				if (M*==M)
					esegui Gc
				else
					esegui Ra
			}
		}
	}
}
if (tipologia==EPLL)
{
	//controllo barra di un lato corretto (per semplificare le U perm)
	if (M*==M)
		case=Uperm
	if (I*==I)
		esegui U’
		case=Uperm
	if (E*==E)
		esegui U2
		case=Uperm
	if (Q*==Q)
		esegui U
		case=Uperm
	//riprendo con l’eseguire il vero pll
	if (case==Uperm)
	{
		if (I*==I)
			sei a posto
		if (I*==-I)
			esegui Ua
		if (I*!=I && I*!=-I)
			esegui Ub
	}
	if (case!=Uperm)
	{
		if (M*==-M)
			esegui H
		else
		{
			if (M*==I)
				esegui Z (l’algoritmo non la rotazione)
			else
				esegui U Z (sempre l’algoritmo)
		}
	}
}
if (tipologia==diag)
{
	if (M*==N)
	{
		if (Q*==Q)
			esegui U’ V
		if (Q*==-Q)
			esegui Na
		if (Q*!=Q && Q*!=-Q)
			esegui U Y (ancora una volta mi riferisco all’algoritmo Y non alla rotazione)
	}
	if (M*==-N*)
	{
		if (I*==J)
			esegui U2 V
		if (I*==-J)
			esegui Nb
		if (I*!=J && I*!=-J)
			esegui U2 Y (ormai lo sai già, da qui in poi non lo dico più)
	}
	if (M*!=N && M*!=-N)
	{
		if (Q*==Q)
			esegui U’ Y
		if (Q*==-Q)
			esegui V
		if (Q*!=Q && Q*!=-Q)
		{
			if (E*==F)
				esegui U V
			if (E*==-F)
				esegui Y
			if (E*!=F && E*!=-F)
			{
				if (E*==I)
					esegui U E
				else
					esegui E
			}
		}
	}
}

//AUF (ultima mossa della risoluzione)

if (E==blu)
	sei a posto
if (E==arancione)
	esegui U’
if (E==verde)
	esegui U2
if (E==rosso)
	esegui U
