//controllo spigoli nell’oll e AUF per dimezzare i casi

if (A==bianco)
{
	if (B==bianco)
	{
		if (C==bianco)
			tipologia=cross
		else
			esegui U’
			tipologia=L
	}
	else
	{
		if (C==bianco)
			esegui U
			tipologia=I
		else
			tipologia=L
	}
}
else
{
	if (B==bianco)
	{
		if (C==bianco)
			esegui U2
			tipologia=L
		else
			tipologia=I
	}
	else
	{
		if (C==bianco)
			esegui U
			tipologia=L
		else
			tipologia=dot
	}
}

